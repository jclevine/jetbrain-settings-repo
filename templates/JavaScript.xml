<templateSet group="JavaScript">
  <template name="us" value="'use strict'&#10;&#10;$END$" description="Inserts 'use strict' statement" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="JS_STATEMENT" value="true" />
    </context>
  </template>
  <template name="it" value="it('$DESCRIBE$', sinon.test(function (done) {&#10;  $END$&#10;}));" description="Unit test template." toReformat="false" toShortenFQNames="true">
    <variable name="DESCRIBE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="try" value="try {&#10;  $END$&#10;} catch (err) {&#10;  &#10;}" description="try/catch block." toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="spec" value="'use strict'&#10;&#10;const sinon = require('sinon')&#10;&#10;const BaseMockSpec = require('./BaseMockSpec')&#10;const baseMockSpec = new BaseMockSpec()&#10;&#10;const expect = require('chai').expect&#10;&#10;baseMockSpec.runSuite('$TEST_OBJECT$',&#10;  {&#10;    '$FIRST_TEST$': function () {&#10;      const testHelper = this.test.testHelper&#10;      &#10;      $END$&#10;    }&#10;  }&#10;)&#10;&#10;" description="Mocha spec." toReformat="false" toShortenFQNames="true">
    <variable name="TEST_OBJECT" expression="capitalize(camelCase(fileNameWithoutExtension()))" defaultValue="" alwaysStopAt="true" />
    <variable name="FIRST_TEST" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="beforeEach" value="beforeEach(() =&gt; {&#10;  $END$&#10;})" description="Creates mocha beforeEach block." toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="console" value="console.log('$END$')" description="Prep console." toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="TODO" value="// TODO: jlevine - $END$" description="My TODO." toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
